{
  "name": "@useoptic/cli",
  "description": "API automation for the modern team",
  "version": "2.3.0",
  "author": "@useoptic",
  "bin": {
    "optic": "./bin/run"
  },
  "bugs": "https://github.com/opticdev/optic-cli/issues",
  "dependencies": {
