    "express": "^4.16.4",
    "find-free-port": "^2.0.0",
    "fs-extra": "^7.0.1",
    "git-state": "^4.1.0",
    "js-yaml": "^3.12.1",
    "keytar": "^4.4.0",
    "nice-try": "latest",
    "request-promise-native": "^1.0.7",
    "tslib": "^1.9.3",
    "unirest": "^0.6.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/archy": "0.0.31",
    "@types/chai": "^4.1.7",
    "@types/debug": "^4.1.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/joi": "^14.3.1",
    "@types/js-yaml": "^3.12.0",
